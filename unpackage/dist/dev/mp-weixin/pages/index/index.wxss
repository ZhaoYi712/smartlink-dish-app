.main {
  background: url("https://tse1-mm.cn.bing.net/th/id/OIP-C.5zGN9Y6AG9GlSMpjxUVkkAHaNK?rs=1&pid=ImgDetMain");
  width: 100%;
  min-height: 100%;
  background-size: cover;
  background-repeat: no-repeat;
}
.container {
  display: flex;
  flex-direction: column;
  padding: 25rpx;
  position: absolute;
  top: 450rpx;
  height: 0;
}
.container .userInfo {
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #ffffff;
  padding: 10rpx;
  box-shadow: 1px 1px 2px 2px rgba(0, 0, 0, 0.05);
  background-color: #4a4a4a;
  opacity: 0.9;
}
.container .userInfo .infoTip {
  color: #FAB714;
  font-size: 28rpx;
}
.swiper {
  width: 100%;
  position: absolute;
}
.swiper .swiper-box {
  height: 550rpx;
}
.swiper .swiper-box image {
  width: 100%;
  height: 550rpx;
}
.ac-option {
  border-radius: 0rpx 0rpx 20rpx 20rpx;
  background-color: #ffffff;
  height: 250rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.ac-option .option-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.ac-option .option-t {
  margin: 10rpx;
  font-weight: bold;
}
.ac-option .option-tip {
  color: #d78f00;
  font-size: 20rpx;
}
.active {
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #ffffff;
  padding: 10rpx;
  padding-left: 50rpx;
  padding-right: 50rpx;
  margin-bottom: 20rpx;
  box-shadow: 1px 1px 2px 2px rgba(0, 0, 0, 0.05);
  justify-content: space-between;
}
.active .index {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 24rpx;
  color: #919293;
}
.active .active-img {
  width: 80rpx;
  height: 80rpx;
  margin: 10rpx;
}
.preferential .title {
  margin: 10rpx;
  color: #414141;
  font-size: 32rpx;
  border-bottom: 2px solid #a4a4a4;
}
.preferential .item {
  border-radius: 20rpx;
  width: 100%;
  height: 300rpx;
  margin-bottom: 10rpx;
}

